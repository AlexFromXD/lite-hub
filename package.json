{
  "name": "api-gateway",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node build.mjs",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:prod": "NODE_ENV=production node build.mjs",
    "format": "prettier --write --log-level warn",
    "format:all": "prettier --write --log-level warn .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http-proxy": "^1.18.1",
    "husky": "^9.1.7",
    "morgan": "^1.10.0",
    "uuid": "^11.1.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.16",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.8.4",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.23.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.10"
  }
}
