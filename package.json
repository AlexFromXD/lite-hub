{
  "name": "api-gateway",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node build.mjs",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:prod": "NODE_ENV=production node build.mjs",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --log-level warn",
    "format:all": "prettier --write --log-level warn .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy": "^1.18.1",
    "husky": "^9.1.7",
    "morgan": "^1.10.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/http-proxy": "^1.17.17",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  }
}
